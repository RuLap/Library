@using Library.HtmlHelpers
@using Microsoft.AspNet.Identity
@model Library.Models.AdminIndexViewModel
@{
    ViewBag.Title = "Админ-панель";
}

<h2>Админка</h2>

<h4>Имя: @Model.FirstName</h4>
<h4>Фамилия: @Model.LastName</h4>
<h4>Email: @Model.Email</h4>
<a class="btn btn-warning" href="@Url.Action("EditUser", new { userId = System.Web.HttpContext.Current.User.Identity.GetUserId()})">Редактировать</a>

<h3><a href="@Url.Action("AllBooksList")">Книги</a></h3>
<h3><a href="@Url.Action("AllUsersList")">Пользователи</a></h3>